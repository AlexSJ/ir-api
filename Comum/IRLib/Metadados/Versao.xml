<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Classe Nome="Versao">
    <Propriedades>
        <Propriedade Nome="Major"						Tipo="int"	/>
        <Propriedade Nome="Minor"						Tipo="int"	/>
        <Propriedade Nome="AtualizacaoObrigatoria"		Tipo="bool"	/>
        <Propriedade Nome="AvisaCliente"				Tipo="bool"	/>
        <Propriedade Nome="Descricao"					Tipo="memo"	/>
        <Propriedade Nome="Obs"							Tipo="memo"	/>
    </Propriedades>
    <Metodos>
        <Metodo Nome="Atualizada" Tipo="bool"><!-- Fabio -->
			Verifica se essa versao esta atualizada ou nao.
			<Parametros>
    			<Parametro Nome="major" Tipo="int" />
    			<Parametro Nome="minor" Tipo="int" />
    		</Parametros>
    	</Metodo>
        <Metodo Nome="Todas" Tipo="DataTable"><!-- Fabio -->
			Obter todas as versoes.
    	</Metodo>
        <Metodo Nome="UltimaVersao" Tipo="void"><!-- Fabio -->
			Obter a ultima versao registrada e atribuir a esta instancia. Se nao houver versoes, devolve erro.
    	</Metodo>
    </Metodos>
</Classe>